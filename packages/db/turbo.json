{
  "extends": ["//"],
  "pipeline": {
    "db:generate": {
      "outputs": ["../../node_modules/.prisma/**"],
      "outputMode": "new-only"
    },
    "db:deploy": {
      "cache": false
    },
    "db:push": {
      "cache": false
    },
    "db:reset": {
      "cache": false
    },
    "db:seed": {
      "dependsOn": ["db:push"],
      "cache": false
    },
    "db:prod:seed": {
      "dependsOn": ["db:push"],
      "cache": false
    },
    "db:studio": {
      "cache": false
    }
  }
}
